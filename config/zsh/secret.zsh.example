# Secret Environment Variables Example
# Copy this file to secret.zsh and fill in your actual values
# secret.zsh is git-ignored for security

# API Keys
export OPENAI_API_KEY="your-openai-api-key-here"
export GEMINI_API_KEY="your-gemini-api-key-here"
export ANTHROPIC_API_KEY="your-anthropic-api-key-here"
export EXA_API_KEY="your-exa-api-key-here"
export BRAVE_API_KEY="your-brave-api-key-here"

# Database connections (uncomment and fill as needed)
